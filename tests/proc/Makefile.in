#EF# hack for avoiding the usage of an old /usr/lib/libkerrighed.so
@BUILD_LIBS_TRUE@libkerrighed: libkerrighed.la libkerrighed_uptodate

@BUILD_LIBS_TRUE@libkerrighed_uptodate:
@BUILD_LIBS_TRUE@	if ! $(MAKE) --question -C $(top_builddir)/../libs/libkerrighed > /dev/null 2>&1 ; then \
@BUILD_LIBS_TRUE@	  rm -f libkerrighed*; \
@BUILD_LIBS_TRUE@	  $(MAKE) libkerrighed.la; \
@BUILD_LIBS_TRUE@	fi

@BUILD_LIBS_TRUE@libkerrighed.la:
@BUILD_LIBS_TRUE@	$(MAKE) -C $(top_builddir)/../libs/libkerrighed install-libLTLIBRARIES libdir=$(PWD) DESTDIR="" && \
@BUILD_LIBS_TRUE@	sed -e 's,^libdir=.*$$,libdir=$$(pwd),' < libkerrighed.la > libkerrighed.la_mod && \
@BUILD_LIBS_TRUE@	mv libkerrighed.la_mod libkerrighed.la
@BUILD_LIBS_TRUE@	touch libkerrighed
